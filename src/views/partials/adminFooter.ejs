<div class="modal fade" id="confirm-del-modal">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header danger">
				<h4 class="modal-title"><i class="fa fa-warning"></i> <span><%= __("Confirm Deletion") %></span></h4>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			</div>
			<div class="modal-body">
				<p><%= __("Are you sure you want to delete this entries?") %></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-danger modal-yes" data-dismiss="modal"><%= __("Yes") %></button>
				<button type="submit" class="btn btn-secondary modal-no" data-dismiss="modal"><%= __("No") %></button>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" id="confirm-info-modal">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title"><i class="fa fa-info-circle"></i> <span></span></h4>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
			</div>
			<div class="modal-body">
				<p></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary modal-ok" data-dismiss="modal"><%= __("Confirm") %></button>
			</div>
		</div>
	</div>
</div>